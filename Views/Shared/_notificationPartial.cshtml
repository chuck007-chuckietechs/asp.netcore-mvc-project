<script>

    var msg =@(TempData["Message"]!=null? Html.Raw(TempData["Message"]): Html.Raw("undefined"));

    console.log(msg);

    if (msg) {
        if (msg.provider == "sweetAlert") {
            swal.fire({
                title: msg.title,
                text: msg.text,
                icon: msg.icon,
                buttons: msg.buttons = true,
                dangerMode: msg.dangerMode = true
            });
        } else {
            toastr[msg.type](msg.text, msg.title)
        }
    }

</script>